#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/src/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/src/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/bundles/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/compiler-cli/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules/@angular/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/@angular+compiler-cli@17.3.2_@angular+compiler@17.3.2_typescript@5.4.3/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
