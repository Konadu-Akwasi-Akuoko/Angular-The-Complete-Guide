#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules/css-prefers-color-scheme/dist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules/css-prefers-color-scheme/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules/css-prefers-color-scheme/dist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules/css-prefers-color-scheme/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/css-prefers-color-scheme@6.0.3_postcss@8.4.31/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../dist/cli.cjs" "$@"
fi
