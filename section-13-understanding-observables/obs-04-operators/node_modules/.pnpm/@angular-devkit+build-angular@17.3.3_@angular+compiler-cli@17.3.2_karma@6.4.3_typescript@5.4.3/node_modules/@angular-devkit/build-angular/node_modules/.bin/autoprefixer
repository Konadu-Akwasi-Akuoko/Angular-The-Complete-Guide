#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules/autoprefixer/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules/autoprefixer/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules/autoprefixer/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules/autoprefixer/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/autoprefixer@10.4.18_postcss@8.4.35/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-04-operators/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../../../autoprefixer/bin/autoprefixer" "$@"
fi
