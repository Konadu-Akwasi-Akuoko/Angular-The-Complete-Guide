#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/esm/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/esm/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules/glob/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/glob@10.3.12/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../glob@10.3.12/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../glob@10.3.12/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
