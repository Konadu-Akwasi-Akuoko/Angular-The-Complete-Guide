#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules/stylus/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules/stylus/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules/stylus/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules/stylus/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/stylus@0.59.0/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../stylus@0.59.0/node_modules/stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../../../../../../stylus@0.59.0/node_modules/stylus/bin/stylus" "$@"
fi
