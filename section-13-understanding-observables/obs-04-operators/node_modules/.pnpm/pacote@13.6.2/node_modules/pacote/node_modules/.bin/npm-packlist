#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/npm-packlist@5.1.3/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../npm-packlist@5.1.3/node_modules/npm-packlist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../npm-packlist@5.1.3/node_modules/npm-packlist/bin/index.js" "$@"
fi
