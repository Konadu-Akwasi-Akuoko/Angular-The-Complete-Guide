#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-13-understanding-observables/obs-01-start/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@5.1.5_less@4.2.0_sass@1.71.1_terser@5.29.1/node_modules/vite/bin/vite.js" "$@"
fi
